<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:rep="internal"
    jcr:primaryType="rep:ACL">
    <deny
        jcr:primaryType="rep:DenyACE"
        rep:principalName="everyone"
        rep:privileges="{Name}[jcr:all]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:glob="/*"/>
    </deny>
    <allow1
        jcr:primaryType="rep:GrantACE"
        rep:principalName="everyone"
        rep:privileges="{Name}[rep:readProperties]"/>
    <allow2
        jcr:primaryType="rep:GrantACE"
        rep:principalName="resourcecollectionservice"
        rep:privileges="{Name}[jcr:read,jcr:modifyAccessControl,jcr:readAccessControl,rep:write]"/>
    <allow3
        jcr:primaryType="rep:GrantACE"
        rep:principalName="reference-adjustment-service"
        rep:privileges="{Name}[jcr:read,jcr:modifyProperties]"/>
    <allow4
        jcr:primaryType="rep:GrantACE"
        rep:principalName="brandportal-imsconfig-service"
        rep:privileges="{Name}[jcr:read,rep:write]"/>
    <allow5
        jcr:primaryType="rep:GrantACE"
        rep:principalName="dam-users"
        rep:privileges="{Name}[jcr:read,rep:write]"/>
</jcr:root>
