<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:fd="http://www.adobe.com/aemfd/fd/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:rep="internal"
    jcr:primaryType="rep:ACL">
    <allow
        jcr:primaryType="rep:GrantACE"
        rep:principalName="fd-service"
        rep:privileges="{Name}[jcr:modifyAccessControl,jcr:readAccessControl]"/>
    <deny1
        jcr:primaryType="rep:DenyACE"
        rep:principalName="everyone"
        rep:privileges="{Name}[jcr:modifyProperties]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:glob="*/related/forms/sling:members/*"
            rep:itemNames="{Name}[eligibilityExpression]"/>
    </deny1>
    <deny2
        jcr:primaryType="rep:DenyACE"
        rep:principalName="everyone"
        rep:privileges="{Name}[jcr:modifyProperties]">
        <rep:restrictions
            fd:condition="[xfaForm,1]"
            jcr:primaryType="rep:Restrictions"
            rep:glob="*/jcr:content/*"
            rep:itemNames="{Name}[xfaForm]"/>
    </deny2>
    <deny3
        jcr:primaryType="rep:DenyACE"
        rep:principalName="everyone"
        rep:privileges="{Name}[jcr:modifyProperties]">
        <rep:restrictions
            fd:condition="[../../../xfaForm,1]"
            jcr:primaryType="rep:Restrictions"
            rep:glob="*/jcr:content/renditions/original/*"
            rep:itemNames="{Name}[jcr:data]"/>
    </deny3>
    <deny4
        jcr:primaryType="rep:DenyACE"
        rep:principalName="everyone"
        rep:privileges="{Name}[jcr:modifyProperties]">
        <rep:restrictions
            fd:condition="[fd:trusted,true]"
            jcr:primaryType="rep:Restrictions"
            rep:glob="/*"
            rep:itemNames="{Name}[fd:trusted]"/>
    </deny4>
    <deny5
        jcr:primaryType="rep:DenyACE"
        rep:principalName="everyone"
        rep:privileges="{Name}[jcr:modifyProperties]">
        <rep:restrictions
            fd:condition="[../fd:trusted,true]"
            jcr:primaryType="rep:Restrictions"
            rep:glob="/*"
            rep:itemNames="{Name}[jcr:data]"/>
    </deny5>
    <allow6
        jcr:primaryType="rep:GrantACE"
        rep:principalName="forms-script-writers"
        rep:privileges="{Name}[jcr:modifyProperties]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:glob="*/related/forms/sling:members/*"
            rep:itemNames="{Name}[eligibilityExpression]"/>
    </allow6>
    <allow7
        jcr:primaryType="rep:GrantACE"
        rep:principalName="forms-xfa-writers"
        rep:privileges="{Name}[jcr:modifyProperties]">
        <rep:restrictions
            fd:condition="[xfaForm,1]"
            jcr:primaryType="rep:Restrictions"
            rep:glob="*/jcr:content/*"
            rep:itemNames="{Name}[xfaForm]"/>
    </allow7>
    <allow8
        jcr:primaryType="rep:GrantACE"
        rep:principalName="forms-xfa-writers"
        rep:privileges="{Name}[jcr:modifyProperties]">
        <rep:restrictions
            fd:condition="[../../../xfaForm,1]"
            jcr:primaryType="rep:Restrictions"
            rep:glob="*/jcr:content/renditions/original/*"
            rep:itemNames="{Name}[jcr:data]"/>
    </allow8>
    <allow9
        jcr:primaryType="rep:GrantACE"
        rep:principalName="forms-xfa-writers"
        rep:privileges="{Name}[jcr:modifyProperties]">
        <rep:restrictions
            fd:condition="[fd:trusted,true]"
            jcr:primaryType="rep:Restrictions"
            rep:glob="/*"
            rep:itemNames="{Name}[fd:trusted]"/>
    </allow9>
    <allow10
        jcr:primaryType="rep:GrantACE"
        rep:principalName="forms-xfa-writers"
        rep:privileges="{Name}[jcr:modifyProperties]">
        <rep:restrictions
            fd:condition="[../fd:trusted,true]"
            jcr:primaryType="rep:Restrictions"
            rep:glob="/*"
            rep:itemNames="{Name}[jcr:data]"/>
    </allow10>
    <allow11
        jcr:primaryType="rep:GrantACE"
        rep:principalName="cm-agent-users"
        rep:privileges="{Name}[jcr:read]"/>
    <allow12
        jcr:primaryType="rep:GrantACE"
        rep:principalName="forms-submission-reviewers"
        rep:privileges="{Name}[jcr:read]"/>
</jcr:root>
